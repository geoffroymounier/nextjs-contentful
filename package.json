{
  "name": "next-js-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "yarn populate:contentful && next build",
    "start": "next start",
    "build-stats": "ANALYZE=true npm run build",
    "export": "next export -o dist",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next dist",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "populate:contentful": "node src/utils/Purge.js",
    "populate:css": "dotenv contentful space migration --management-Token %MANAGEMENT_TOKEN% --space-id %CONTENTFUL_SPACE% src/utils/Purge.js --y"
  },
  "dependencies": {
    "@magic-sdk/admin": "^1.3.0",
    "@sanity/block-content-to-react": "2.0.7",
    "@sanity/image-url": "0.140.22",
    "autoprefixer": "^10.0.4",
    "classnames": "^2.2.6",
    "final-form": "~4.20.2",
    "firebase-admin": "^9.4.2",
    "fs": "^0.0.1-security",
    "jsoneditor": "9.1.9",
    "jsoneditor-react": "3.1.0",
    "mailgun-js":"0.22.0",
    "magic-sdk": "^4.0.2",
    "next": "^10.0.3",
    "next-sanity-image": "1.0.1",
    "next-seo": "^4.17.0",
    "postcss": "^8.1.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-final-form": "~6.5.3",
    "react-html-parser": "^2.0.2",
    "react-lazyload": "^3.1.0",
    "react-svg": "~12.1.0",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.0.1",
    "yup": "~0.32.9"
  },
  "license": "ISC",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@next/bundle-analyzer": "^10.0.3",
    "@sanity/client": "2.2.6",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "cross-var": "1.1.0",
    "css-loader": "~5.0.1",
    "dotenv": "8.2.0",
    "dotenv-cli": "4.0.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "3.0.0",
    "lint-staged": "^10.5.1",
    "lodash": "4.17.20",
    "nanoid": "3.1.20",
    "nodemon": "2.0.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "purgecss": "3.0.0",
    "react-icons": "^3.11.0",
    "rimraf": "^3.0.2",
    "style-loader": "~2.0.0",
    "typescript": "^4.0.5",
    "url-loader": "~4.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "eslint"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
